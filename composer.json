{
    "name": "phonetworks/pho-server-rest",
  "type": "library",
  "description": "A REST based management API for the Pho Kernel.",
   "keywords": [
        "graph",
        "server",
        "api",
        "rest",
        "reactphp",
        "react",
        "pho"
    ],
  "homepage": "https://phonetworks.org",
  "license": "MIT",
  "authors": [
        {
            "name": "Emre Sokullu",
            "email": "emre@phonetworks.org",
            "homepage": "http://www.emresokullu.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php":">=7.1.0",
        "esokullu/react-restify": "dev-master",
        "phonetworks/pho-kernel": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "fzaninotto/faker": "^1.6",
        "psy/psysh": "*",
        "squizlabs/php_codesniffer": "^2.5",
	"roave/security-advisories": "dev-master",
        "guzzlehttp/guzzle": "^6.3"
    },
     "autoload": {
	     "psr-4": {
		       "Pho\\Server\\Rest\\": "src/Pho/Server/Rest/"
	     }
    },
    "autoload-dev": {
	     "psr-4": {
		       "": "tests/"
	     }
    }
}
